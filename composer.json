{
    "name": "samdark/yiiframework-ru",
    "description": "yiiframework.ru",
    "keywords": [
        "yii",
        "yiiframework.ru"
    ],
    "homepage": "http://www.yiiframework.ru/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/samdark/yiiframework-ru/issues?state=open",
        "forum": "http://yiiframework.ru/forum/",
        "source": "https://github.com/samdark/yiiframework-ru/"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "~2.0.35",
        "yiisoft/yii2-bootstrap": "~2.0.8",
        "yiisoft/yii2-swiftmailer": "~2.1.1",
        "yiisoft/yii2-authclient": "~2.2.7",
        "kartik-v/yii2-widget-fileinput": "~1.0.6",
        "creocoder/yii2-taggable": "~2.0.0",
        "lavrentiev/yii2-toastr": "2.0.0",
        "baibaratsky/yii2-rollbar": "~1.8.0",
        "bower-asset/codemirror": "~5.9.0",
        "bower-asset/codemirror-buttons": "~1.0.4"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.13",
        "yiisoft/yii2-gii": "~2.2.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
